{
    "name": "stest",
    "displayName": "stest",
    "description": "serverside communication test",
    "version": "0.0.1",
    "publisher": "f",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.submit",
        "onCommand:extension.highlight",
        "onCommand:extension.login"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.submit",
            "title": "submit"
        },
        {
            "command": "extension.highlight",
            "title": "highlight"
        },
        {
            "command": "extension.login",
            "title": "login"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "https": "latest",
        "axios": "^0.15.3",
        "escape-markdown": "^1.0.0",
        "follow-redirects": "^1.0.0",
        "markdown-escape": "^1.0.0",
        "path": "^0.12.7",
        "process": "^0.11.9"
    },
    "contributes": {
        "keybindings": [
            {
                "key": "shift+cmd+h",
                "command": "extension.submit"
            },
            {
                "key": "shift+cmd+b",
                "command": "extension.highlight"
            },
            {
                "key": "shift+cmd+l",
                "command": "extension.login"
            }
        ]
    }
}