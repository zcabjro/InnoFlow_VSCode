{
    "name": "stest",
    "displayName": "stest",
    "description": "serverside communication test",
    "version": "0.0.1",
    "publisher": "f",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.ctest"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        },
        {
            "command": "extension.ctest",
            "title": "stest"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "contributes": {
        "keybindings": [
            {
                "key": "shift+cmd+h",
                "command": "extension.sayHello"
            },
            {
                "key": "shift+cmd+b",
                "command": "extension.ctest"
            }
        ]
    }
}